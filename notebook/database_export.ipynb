       "  'ClipBounds': [63.263397216796875,\n",
       "   600.9173889160156,\n",
       "   534.4932556152344,\n",
       "   643.0507049560547],\n",
       "  'Font': {'alt_family_name': 'Calibri',\n",
       "   'embedded': True,\n",
       "   'encoding': 'Custom',\n",
       "   'family_name': 'Calibri',\n",
       "   'font_type': 'TrueType',\n",
       "   'italic': False,\n",
